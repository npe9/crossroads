#processor block decomposition
sms(4,2,2)
#Always specify blocks in block base numbering
blk(on,0:3,0:1,0:1)

# tag boundary faces
tag("xMinFaces",face,(0:0,0:2,0:2))
tag("xMaxFaces",face,(4:4,0:2,0:2))
tag("yMinFaces",face,(0:4,0:0,0:2))
tag("yMaxFaces",face,(0:4,2:2,0:2))
tag("zMinFaces",face,(0:4,0:2,0:0))
tag("zMaxFaces",face,(0:4,0:2,2:2))

# define number of zones in each axis
numzones(3,6,8)

#Hex subdivisions
sub(10%,0:3, 0:1, 0:1,(7,0,0,0)) #7 hex
seed(10)

